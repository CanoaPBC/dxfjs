{
	"name": "@tarikjabiri/dxf",
	"version": "2.3.1",
	"description": "A JavaScript interface to Dxf written in TypeScript.",
	"main": "./lib/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/index.d.ts",
	"repository": "https://github.com/tarikjabiri/dxf",
	"author": "Tarik EL JABIRI",
	"license": "MIT",
	"scripts": {
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"build": "rollup -c",
		"example": "node examples/example.js",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:serve": "vitepress serve docs",
		"lint": "eslint src --fix",
		"prettier": "prettier --write .",
		"dev": "esbuild src/index.ts --format=cjs --bundle --outfile=dist/index.js"
	},
	"files": [
		"lib/**/*"
	],
	"devDependencies": {
		"@rollup/plugin-json": "^4.1.0",
		"@types/node": "^18.7.2",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"c8": "^7.12.0",
		"del": "^7.0.0",
		"esbuild": "^0.15.1",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "2.7.1",
		"rollup": "^2.77.3",
		"rollup-plugin-dts": "^4.2.2",
		"rollup-plugin-typescript2": "^0.32.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vitepress": "1.0.0-alpha.8",
		"vitest": "^0.21.1",
		"vue": "^3.2.37"
	},
	"funding": "https://github.com/sponsors/dxfjs",
	"keywords": [
		"js-dxf",
		"ts-dxf",
		"dxf",
		"writer",
		"js",
		"ts"
	],
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@algolia/client-search"
			]
		}
	}
}
